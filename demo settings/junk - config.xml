<?xml version="1.0" encoding="UTF-8"?>

   <config version="5.0" htmlParse="false">

   <!-- GENERAL SETTINGS -->
   <!-- "setupTarget" options: Netlify, firebase, local, email, or sheets -->
   <item id="studyTitle">my Q study</item> 
   <item id="databaseOptions">Include_Google_Options</item>
   <item id="setupTarget">netlify</item>
   <!-- STEIN -->
   <item id="steinApiUrl"></item>
   <!-- EMAIL -->
   <item id="emailAddress"></item>  
   <item id="emailSubject">EQ Web Sort Results</item>  
   <!-- SECOND PROJECT -->
   <item id="linkToSecondProject">false</item> 
   <item id="secondProjectUrl"></item> 
   <!-- IMAGE OPTIONS  (imageFormat: 169 or 43)-->
   <item id="useImages">true</item>
   <item id="numImages">0</item>
   <item id="imageFileType">jpg</item>
   <item id="imageFormat">postSortImageModal169</item>
   <item id="shuffleImages">true</item>
   <!-- OTHER GENERAL OPTIONS -->
   <item id="shuffleCards">true</item>
   <item id="headerBarColor">#337ab7</item>
   <item id="devMode">false</item> 

   <!-- ACCESS -->
   <!-- options: anonymous, partId-access, partId, access -->
   <item id="initialScreen">anonymous</item>
   <item id="accessCode">demo</item>

   <!-- LEGACY DEFAULTS (do not change these)-->
   <item id="setDefaultFontSize">true</item>
   <item id="defaultFontSize">25</item>
   <item id="displayNeutralObjects">false</item>

   
   <!-- PRESORT -->
   <!-- default colors #2a2a2a, #ccffcc, #e0e0e0, #ffe0f0 -->
   <item id="greenCardColor">#ccffcc</item>
   <item id="yellowCardColor">#e0e0e0</item>
   <item id="pinkCardColor">#ffe0e0</item>
   <item id="defaultFontColor">#2a2a2a</item>

   <item id="setDefaultFontSizePresort">true</item>
   <item id="defaultFontSizePresort">22</item>
  

   <!-- SORT -->
   <item id="allowUnforcedSorts">false</item>
   <item id="warnOverloadedColumn">true</item>
   <item id="condOfInstFontSize">20</item>

   <item id="setMinCardHeightSort">true</item>
   <item id="minCardHeight">120</item>
   <item id="setDefaultFontSizeSort">true</item>
   <item id="defaultFontSizeSort">16</item>
   
   <item id="sortDirection">positive</item> 

   <!-- POSTSORT -->
   <item id="showPostsort">true</item>
   <item id="showSecondPosColumn">false</item>
   <item id="showSecondNegColumn">false</item>
   <item id="showBackButton">true</item>
   <item id="postsortCommentsRequired">false</item>

   <item id="setDefaultFontSizePostsort">true</item>
   <item id="defaultFontSizePostsort">16</item>
   <item id="setMinCardHeightPostsort">true</item>
   <item id="minCardHeightPostsort">120</item>


   <!-- SURVEY - Survey Questions -->
   <item id="showSurvey">true</item>


       
  </config>